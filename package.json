{
  "name": "package-template",
  "version": "0.0.1",
  "description": "npm package development template",
  "author": "Senao Xi",
  "keywords": [
    "typescript"
  ],
  "license": "MIT",
  "packageManager": "pnpm@8.5.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc --noEmit && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "dev": "rollup -c rollup.config.ts -w",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest watch --ui",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.3",
    "@babel/runtime": "^7.23.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/babel__plugin-transform-runtime": "^7.9.5",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/node": "^20.2.3",
    "@types/rollup-plugin-node-builtins": "^2.1.5",
    "@types/rollup-plugin-node-globals": "^1.4.4",
    "@vitest/ui": "^0.31.1",
    "c8": "^7.13.0",
    "lodash.camelcase": "^4.3.0",
    "minifaker": "^1.34.1",
    "rollup": "^4.6.0",
    "rollup-plugin-gzip": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tslib": "^2.6.2",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "directories": {
    "test": "tests"
  }
}
